                    {
                        if callvalue() { revert(_2, _2) }
                        if slt(add(calldatasize(), not(3)), _2) { revert(_2, _2) }
                        let _3 := datasize("C_2")
                        let _4 := add(_1, _3)
                        if or(gt(_4, 0xffffffffffffffff), lt(_4, _1)) { invalid() }
                        datacopy(_1, dataoffset("C_2"), _3)
                        pop(create(_2, _1, sub(_4, _1)))
                        return(allocateMemory(_2), _2)
