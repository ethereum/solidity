    (memory $memory (export "memory") 1)
    (export "main" (func $main))

(func $main
    (local $_1 i64)
    (local.set $_1 (i64.const 0))
    (call $mstore_internal (i64.const 0) (local.get $_1) (local.get $_1) (local.get $_1) (local.get $_1))
